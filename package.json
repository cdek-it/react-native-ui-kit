{
  "name": "@cdek/react-native-prime-ui-kit",
  "packageManager": "yarn@4.6.0",
  "version": "0.0.23",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "test": "jest",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "storybook-generate": "sb-rn-get-stories --config-path .storybook",
    "doctor": "expo-doctor",
    "check": "expo install --check"
  },
  "devDependencies": {
    "@babel/core": "7.26.7",
    "@babel/preset-env": "7.26.7",
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@expo/metro-runtime": "4.0.1",
    "@gorhom/bottom-sheet": "5.0.6",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/slider": "4.5.5",
    "@storybook/addon-actions": "8.3.5",
    "@storybook/addon-controls": "8.3.5",
    "@storybook/addon-ondevice-actions": "8.3.5",
    "@storybook/addon-ondevice-controls": "8.3.5",
    "@storybook/addon-ondevice-notes": "8.3.5",
    "@storybook/core": "8.3.5",
    "@storybook/preview-api": "8.3.5",
